{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug http",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "https://browser.engineering/examples/example1-simple.html"
      ]
    },
    {
      "name": "Debug file",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "file:///D:/"
      ]
    },
    {
      "name": "Debug data",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "data:text/html,Hello world!"
      ]
    },
    {
      "name": "Debug view-source",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "view-source:http://example.org/"
      ]
    },
    {
      "name": "Debug redirect",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "http://browser.engineering/redirect"
      ]
    },
    {
      "name": "Debug chinese site",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "https://browser.engineering/examples/xiyouji.html"
      ]
    },
    {
      "name": "Debug malformed",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "htps:/browser.engineering/examples/xiyouji.html"
      ]
    },
  ]
}